#headerfixed{
    display:flex;
    width:100%;
    background-color:rgb(37, 37, 37);
    position:fixed;
    left:0;
    top:0;
    padding-bottom:50px;
}
#headerfixed ul{
    position:static;
    padding-left:300px;    
}
#headerfixed li{
    padding: 1em 0 0 1em;
    list-style-type:none;
    color:rgb(192, 173, 0);
}
.mainheader{
    width:100%;
    height:960px;
    background-image:url("tarologa.jpg");
    background-repeat: no-repeat;
    background-size:100%;
}
.mainheader{
    display:flex;
    width:100%;
    padding-bottom:50px;
}
.mainheader #retangulo{
    display:flex;
    background-color:white;
    width:50px;
    height:10px;
    position:static;
    margin-left:50px;
    margin-top:50px;
    margin-right:50px;
}
.mainheader h1{
    display:flex;
    color:rgb(192, 173, 0);
    text-decoration: underline;
    text-decoration-color:black;
    -webkit-text-stroke: 5px rgb(0, 0, 0);
    font-size:12em;
    position:static;
    padding-top:600px;
    margin-left:0px;
    width:100%;
}
main{
    display:flex;
    background-color:white;
}
main ul{
    display:flex;
    padding:5%;
}
main li{
    padding-left:5%;
}
.lista{
    display:flex;
    width:100%;
}
.lista img{
    width: 200px;
    height: 200px;
}
